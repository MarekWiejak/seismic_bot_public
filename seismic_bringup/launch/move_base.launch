<launch>
    <include file="$(find seismic_bringup)/launch/amcl.launch"/>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <param name="base_global_planner" value="global_planner/GlobalPlanner"/>
        <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS"/>
        <param name="controller_frequency" value="5.0"/>
        <rosparam file="$(find seismic_bringup)/config/move_base/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
        <rosparam file="$(find seismic_bringup)/config/move_base/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find seismic_bringup)/config/move_base/local_costmap_params.yaml" command="load" />   
        <rosparam file="$(find seismic_bringup)/config/move_base/global_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find seismic_bringup)/config/move_base/dwa_local_planner_params.yaml" command="load" />
    </node>

</launch>